pragma.enable("easy-return")
pragma.disable("explicit-result-guard")

def Runnable extends __makeGuard(Runnable) {
  to coerce(specimen, optEjector) {
    if (!specimen.__respondsTo("run", 0)) {
      traceln("java.lang.Runnable guard could reject " + E.toQuote(specimen))
    }
    return specimen
  }
}