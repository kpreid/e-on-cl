--- modPow ---

? t("a ** b %% c")
# stdout: ModPowE.
#           NounE. a
#           NounE. b
#           NounE. c

Parentheses disable modPow form
? t("(a ** b) %% c")
# stdout: BinaryE.
#           %%
#           BinaryE.
#             **
#             NounE. a
#             [] NounE. b
#           [] NounE. c

? t("a %% b ** c")
# stdout: BinaryE.
#           %%
#           NounE. a
#           [] BinaryE.
#             **
#             NounE. b
#             [] NounE. c

? t("a ** b % c")
# stdout: BinaryE.
#           %
#           BinaryE.
#             **
#             NounE. a
#             [] NounE. b
#           [] NounE. c

? t("(a %% b) ** c")
# stdout: BinaryE.
#           **
#           BinaryE.
#             %%
#             NounE. a
#             [] NounE. b
#           [] NounE. c

? t("a %% (b ** c)")
# stdout: BinaryE.
#           %%
#           NounE. a
#           [] BinaryE.
#             **
#             NounE. b
#             [] NounE. c

