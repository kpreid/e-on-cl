  ? def makeWeakRef := <unsafe:org.erights.e.elib.vat.makeWeakRef>
  # value: <makeWeakRef>
  
  ? <unsafe:org.erights.e.elib.vat.WeakPtr>
  # value: <makeWeakRef>
  
  ? def x {}
  # value: <x>
  
  ? def weakRef := makeWeakRef(x, null)
  # value: <weak:<x>>
  
  ? weakRef.get() == x
  # value: true
  
  ? def x := 44
  # value: 44
  
  XXX separate test for interp.gc()
  ? interp.gc()
  
  x ? [weakRef.get()]
  x # value: [null]
  x 
  x ? weakRef
  x # value: <dead weak ref>
