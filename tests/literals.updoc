String#replaceAll/2

  ? "a".replaceAll("a", "b")
  # value: "b"

  ? "a".replaceAll("a", "bb")
  # value: "bb"

  ? "abc".replaceAll("b", "c")
  # value: "acc"

  ? "babcb".replaceAll("b", "c")
  # value: "caccc"

  ? "abcba".replaceAll("b", "!_!")
  # value: "a!_!c!_!a"

  ? "a".replaceAll("", "b")
  # problem: replaceAll: search string may not be empty

  ? "".replaceAll("a", "b")
  # value: ""

  ? "foobarbarbarfoof".replaceAll("bar", "qx")
  # value: "fooqxqxqxfoof"

split

  ? "".split("<>")
  # value: [""]

  ? "abc".split("<>")
  # value: ["abc"]

  ? "abc<>def".split("<>")
  # value: ["abc", "def"]

  ? "<>abc<>".split("<>")
  # value: ["", "abc", ""]

String#add/1

  ? "ab" + "bc"
  # value: "abbc"
  
  ? "ab" + 10
  # value: "ab10"
  
  ? "ab" + ["cd"]
  # value: "ab[\"cd\"]"

